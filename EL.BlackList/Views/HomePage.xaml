<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EL.BlackList.Views.HomePage"
             xmlns:vm="clr-namespace:EL.BlackList.ViewModels"
             xmlns:uc="clr-namespace:EL.BlackList.UserControl"
             Title="{Binding Title}">
    <ContentPage.BindingContext>
        <vm:HomeViewModel />
    </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout Orientation="Vertical">
            <uc:PaymentView x:Name="PayModel"/>
            <uc:SerchDriverView x:Name="SerchModel" />
            <Label Text="{Binding SerchVisible}" TextColor="Black"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>